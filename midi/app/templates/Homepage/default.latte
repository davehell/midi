
{block jumbotron}
<div id="carousel-wrapper">
  <div id="carousel" class="carousel slide container" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#carousel" data-slide-to="0" class="active"></li>
      <li data-target="#carousel" data-slide-to="1"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
      <div class="item active">
        <h1>MIDI a KARAOKE</h1>
        <div class="carousel-caption">
          <p>{$pocet} skladeb</p>
          <p>cena od 30 Kč</p>
          <p><a n:href="Skladba:default" class="btn btn-primary btn-lg" role="button">Katalog skladeb</a></p>
        </div>
      </div>

      <div class="item">
        <h1>Karaoke na DVD</h1>
        <div class="carousel-caption">
          <p>Skladby je možné zpracovat jako karaoke pro přehrávání v DVD přehrávačích.</p>
          <p>cena 30 Kč</p>
        </div>
      </div>
    </div>

    <!-- Controls -->
    <a class="left carousel-control" href="#carousel" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left"></span>
    </a>
    <a class="right carousel-control" href="#carousel" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right"></span>
    </a>
  </div>
</div>
{/block}

{block content}

<div id="boxy">
  <div class="col-md-3 col-md-offset-1">
  <img data-src="holder.js/140x140" class="center-block img-circle" alt="140x140" src="data:image/png;base64,">
  <span class="glyphicon glyphicon-music"></span>
  <h3>Formáty</h3>
  <ul>
    <li>Karaoke na DVD</li>
    <li n:foreach="$formaty as $format">{$format}</li>
  </ul>
  </div>

  <div class="col-md-3">
  <img data-src="holder.js/140x140" class="center-block img-circle" alt="140x140" src="data:image/png;base64,">
  <span class="glyphicon glyphicon-fire"></span>
  <h3>Novinky</h3>
  <ul n:inner-foreach="$novinky as $skladba">
    <li><a n:href="Skladba:detail $skladba->id">{$skladba->nazev}</a></li>
  </ul>
  </div>

  <div class="col-md-3">
  <img data-src="holder.js/140x140" class="center-block img-circle" alt="140x140" src="data:image/png;base64,">
  <span class="glyphicon glyphicon-heart"></span>
  <h3>Oblíbené</h3>
  <ul n:inner-foreach="$oblibene as $skladba">
    <li><a n:href="Skladba:detail $skladba->skladba->id">{$skladba->skladba->nazev}</a></li>
  </ul>
  </div>
</div>


